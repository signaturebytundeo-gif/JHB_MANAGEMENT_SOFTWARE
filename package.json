{
  "name": "jhb-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev:command-center": "npm run dev --workspace=apps/command-center",
    "dev:ecommerce": "npm run dev --workspace=apps/ecommerce",
    "build:command-center": "npm run build --workspace=apps/command-center",
    "build:ecommerce": "npm run build --workspace=apps/ecommerce",
    "build": "npm run build --workspaces"
  }
}
